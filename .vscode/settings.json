{
  "editor.rulers": [100],
  "editor.formatOnSave": true,
  "files.exclude": {
    "dist": true,
    "out": true,
    "pack": true,
    "node_modules": true,
    "coverage": true,
    ".vscode-test": true
  },
  "typescriptHero.imports.organizeOnSave": true,
  "typescriptHero.imports.multiLineWrapThreshold": 100,
  "prettier.requireConfig": true,
  "versionlens.showVersionLensesAtStartup": false
}
